<?xml version="1.0" encoding="utf-8" ?>
<ProxyConfig allowedReferers="*"
             mustMatch="true">
    <serverUrls>

<serverUrl url="http://landsat.arcgis.com/arcgis/rest/services/Landsat8_PanSharpened/ImageServer"
              matchAll="true"
          username="WRI_LANDSAT_USERNAME"
          password="WRI_LANDSAT_PASSWORD"
          />

  <serverUrl url="http://gis-gfw.wri.org/arcgis/rest/services/Fires/fire_stories/FeatureServer"
          matchAll="true"
          username="GFW_STORY_EDITOR"
          password="GFW_STORY_PASS"
          />

   <serverUrl url="http://gis-gfw.wri.org/arcgis/rest/services/Fires/fire_stories/MapServer"
          matchAll="true"
          username="GFW_STORY_EDITOR"
          password="GFW_STORY_PASS"
          />

    <serverUrl url="http://gis-gfw.wri.org/arcgis/rest/services/protected_services/MapServer"
          matchAll="true"
          username="GFW_BR_USERNAME"
          password="GFW_BR_PASSWORD"
          />

    <serverUrl url="https://gis-gfw.wri.org/arcgis/rest/services/protected_services/MapServer"
          matchAll="true"
          username="GFW_BR_USERNAME"
          password="GFW_BR_PASSWORD"
          />

    <serverUrl url="http://gis-gfw.wri.org/arcgis/rest/services/cached/wdpa_protected_areas/MapServer"
          matchAll="true"
          username="GFW_BR_USERNAME"
          password="GFW_BR_PASSWORD"
          />

    </serverUrls>
</ProxyConfig>

